<?xml version='1.0' encoding='UTF-8'?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	version="2.0">
	<application>
		<el-resolver>
			org.springframework.web.jsf.el.SpringBeanFacesELResolver
		</el-resolver>

		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>zh</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>com.sree.i18N.ResourceBundltUtil</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>

	<lifecycle>
		<phase-listener>com.sree.base.web.PhaseTracker</phase-listener>
	</lifecycle>

</faces-config>